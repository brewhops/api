{
  "version": 2,
  "name": "api",
  "env": {
    "IS_NOW": "true",
    "DB_USER": "@db_user",
    "DB_PASSWORD": "@db_password",
    "DB_HOST": "@db_host",
    "DB_PORT": "@db_port",
    "DB_DEV_USER": "@db_dev_user",
    "DB_DEV_PASSWORD": "@db_dev_password",
    "DB_DEV_HOST": "@db_dev_host",
    "ALGORITHM": "@encrypt_algorithm",
    "KEY": "@encrypt_key",
    "IV": "@encrypt_iv"
  },
  "builds": [{ "src": "src/index.ts", "use": "@now/node@canary" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.ts"
    }
  ]
}